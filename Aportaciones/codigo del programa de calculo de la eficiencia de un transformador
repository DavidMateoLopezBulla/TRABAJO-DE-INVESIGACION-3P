#include<stdio.h>
#include <math.h>
//eficiencia del un transformador

int main()
{
	float pe,ps,ef;
	printf("\n              Eficiencia de un transformador");
	printf("\n----------------------------------------------------------------");
	printf("\n  Ingrese la potencia de entrada :  ");
	scanf("%f", &pe);
	printf("\n  Ingrese la potencia de salida:  ");
	scanf("%f", &ps);
	//calculo de la eficiencia
	ef=ps*100/pe;
    printf("\n La eficiencia del transformador es del %f%%   ", ef);

	 return 0;

}

